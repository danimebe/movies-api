{
    "name": "platzivideo",
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "DB_USER": "@platzivideo-db-user",
        "DB_PASSWORD": "@platzivideos-db-password",
        "DB_HOST": "@platzivideos-db-host",
        "DB_NAME": "@platzivideos-db-name"
    }
}